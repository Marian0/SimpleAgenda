{% extends 'SimpleAgendaBundle:Layout:base.html.twig' %}
{% block title %}CRUD Contacto{% endblock %}
{% block content %}


<h1>Contact creation</h1>

<form action="{{ path('contact_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('contact') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}